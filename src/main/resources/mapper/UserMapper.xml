		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ming.dao.UserDao">
	
	
	<insert id="insert" parameterType="com.ming.domain.User">
		INSERT INTO user (id, password)
		VALUES (#{id}, #{password})
	</insert>
	
